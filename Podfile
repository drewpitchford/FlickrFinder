 platform :ios, '12.0'

target 'FlickrFinder' do
  use_frameworks!
  pod 'AFNetworking'
  pod 'DPKitchenSink'
  pod 'SVProgressHUD'
  pod 'SDWebImage'
  pod 'DataManager'
  pod 'SimpleImageViewer', :git => 'https://github.com/drewpitchford/SimpleImageViewer.git', :branch => 'master'

  target 'FlickrFinderTests' do
    inherit! :search_paths
  end

end
